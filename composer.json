{
    "name": "hardworm/yandex-xml-library",
    "description": "Un-Official Yandex-XML PHP library",
    "keywords":["yandex", "yandex.xml", "yandex xml", "sdk"],
    "type":"library",
    "license": "MIT",
    "version": "1.1.0",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "replace": {
        "anton-shevchuk/yandex-xml-library": "1.*"
    },
    "autoload": {
        "psr-4": {
            "hardworm\\YandexXml\\": "src/YandexXml"
        }
    },
    "authors": [
